#include<stdio.h>
int main(){
	int n,i,max,min,diff;
	scanf("%d",&n);
	int a[n];
	for(i=1;i<n;i++)
	scanf("%d",&a[i]);
	max=a[0];
	min=a[0];
	for(i=1;i<n;i++)
	if(a[i]>max)
		max=a[i];
	{
		printf("%d",max);
	}
	if(a[i]<min)
	    min=a[i];
	{
		printf("%d",min);
	}
	diff=max-min;
	printf("%d",diff);
	return 0;
}
